{
    "expo": {
        "name": "CardVault",
        "slug": "cardvault",
        "version": "1.6.3",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#000"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.bhaumik.tandan.cardvault",
            "buildNumber": "14"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/icon.png",
                "backgroundColor": "#000"
            },
            "package": "com.bhaumik.tandan.cardvault",
            "versionCode": 14
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "extra": {
            "eas": {
                "projectId": "f26cf6f9-5809-4aa2-bf74-370cdbacf72e"
            }
        },
        "owner": "cardvault",
        "plugins": [
            [
              "expo-camera",
              {
                "cameraPermission": "The app needs to access your camera to scan the card"
              }
            ],
            [
              "expo-image-picker",
              {
                "photosPermission": "The app needs to access your photos to get the card details from your galary"
              }
            ]
          ]
    },
    "react-native-google-mobile-ads": {
        "android_app_id": "ca-app-pub-5499479031752321~2533055801",
        "ios_app_id": "ca-app-pub-5499479031752321~1051983983"
    }
}
